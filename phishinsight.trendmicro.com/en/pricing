<script>
  // to ja users, you should not pass/landing this page
  var pathComponents = window.location.href.split('/');
  if (pathComponents.length > 3 && (pathComponents[3] === 'ja' || pathComponents[3] === 'ja-jp')) {
    window.location.replace(pathComponents[0] + '//' + pathComponents[2] + '/' + pathComponents[3] + '/');
  }

  // pricing mobile tabs
  function addClassName(elem, name) {
    var names = elem.className.split(" ");
    if (names.indexOf(name) == -1) {
      elem.className += " " + name;
    }
  }
  function removeClassName(elem, name) {
    var reg = new RegExp('\\b' + name + '\\b', 'g');
    elem.className = elem.className.replace(reg, "");
  }
  function changePricingTab(planId) {
    var tabs = document.getElementsByClassName('landing-pricing-plan-tabs-item');
    for (var i = 0; i < tabs.length; i++) {
      removeClassName(tabs[i], "active");
    }
    var tab = document.getElementById(planId + '-tab');
    addClassName(tab, "active");

    var plans = document.getElementsByClassName('landing-pricing-plan-container');
    for (var i = 0; i < plans.length; i++) {
      removeClassName(plans[i], "active");
    }
    var plan = document.getElementById(planId);
    addClassName(plan, "active");
  }
</script>

