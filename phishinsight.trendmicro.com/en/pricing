<script type="text/javascript">
  (function() {
    function getQueryParameterByName(name) {
        var url = window.location.href;
        name = name.replace(/[\[\]]/g, '\\$&');
        var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, ' '));
    }

    var showModal = getQueryParameterByName('domain_verified_result');
    var modal = document.getElementById('verify-domain-result-modal');
    if (showModal === "yes") {
      modal.className += ' active';
      document.getElementById('verify-domain-msg').innerHTML = "The email domain of your account has been successfully verified and your account has been upgraded to Standard.";
      document.getElementById('verify-domain-confirm-info-icon').src = "/static/img/campaign/confirm_icon.8a6bf8d30de3.png";
    } else if (showModal === "no") {
      modal.className += ' active';
      document.getElementById('verify-domain-msg').innerHTML = "The link on the verification email has expired, please send another and try again.";
      document.getElementById('verify-domain-confirm-info-icon').src = "/static/img/campaign/cross_icon.c88f0e57a745.png";
    }

  })();

  function closeVerifyDomainResultModal() {
    var modal = document.getElementById('verify-domain-result-modal');
    modal.className = modal.className.replace(/\bactive\b/g, "");
  }
</script>


  



