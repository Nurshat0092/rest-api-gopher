<script>
  function scrollToTop() {
    document.body.scrollTop = 0; // For Safari
    document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
  }

  function addClassName(elem, name) {
    var names = elem.className.split(" ");
    if (names.indexOf(name) == -1) {
      elem.className += " " + name;
    }
  }

  function removeClassName(elem, name) {
    var reg = new RegExp('\\b' + name + '\\b', 'g');
    elem.className = elem.className.replace(reg, "");
  }

  function quizReveal(elemId) {
    if (window.sr && window.sr.isSupported()) {
      var elem = document.getElementById(elemId);
      window.sr.reveal(elem, {
        reset: true,
      });
    }
  }

  function quizAnswerPhishing() {
    var question = document.getElementById('landing-simulator-quiz-info-question');
    addClassName(question, 'landing-quiz-hide');

    var answerPhishing = document.getElementById('landing-simulator-quiz-info-phishing');
    removeClassName(answerPhishing, 'landing-quiz-hide');

    var answerBack = document.getElementById('landing-simulator-quiz-answer-back');
    removeClassName(answerBack, 'landing-quiz-hide');

    quizReveal('landing-simulator-quiz-info-phishing');
  }

  function quizAnswerLegitimate() {
    var question = document.getElementById('landing-simulator-quiz-info-question');
    addClassName(question, 'landing-quiz-hide');

    var answerLegitimate = document.getElementById('landing-simulator-quiz-info-legitimate');
    removeClassName(answerLegitimate, 'landing-quiz-hide');

    var answerBack = document.getElementById('landing-simulator-quiz-answer-back');
    removeClassName(answerBack, 'landing-quiz-hide');

    quizReveal('landing-simulator-quiz-info-legitimate');
  }

  function quizAnswerBack() {
    var answerPhishing = document.getElementById('landing-simulator-quiz-info-phishing');
    addClassName(answerPhishing, 'landing-quiz-hide');
    var answerLegitimate = document.getElementById('landing-simulator-quiz-info-legitimate');
    addClassName(answerLegitimate, 'landing-quiz-hide');
    var answerBack = document.getElementById('landing-simulator-quiz-answer-back');
    addClassName(answerBack, 'landing-quiz-hide');
    var question = document.getElementById('landing-simulator-quiz-info-question');
    removeClassName(question, 'landing-quiz-hide');
  }

  function showSaasVideoModal() {
    var modal = document.getElementById('landing-saas-video-modal');
    removeClassName(modal, 'landing-simulator-hide');
    removeClassName(modal, 'fade-out');
    addClassName(modal, 'fade-in');
    addClassName(document.body, 'landing-simulator-modal-show');
  }

  function hideSaasVideoModal() {
    var modal = document.getElementById('landing-saas-video-modal');
    removeClassName(modal, 'fade-in');
    addClassName(modal, 'fade-out');
    removeClassName(document.body, 'landing-simulator-modal-show');

    // pause video
    var video = document.getElementById('landing-youtube-video');
    video.contentWindow.postMessage('{"event":"command","func":"' + 'pauseVideo' + '","args":""}', '*');
  }
</script>
