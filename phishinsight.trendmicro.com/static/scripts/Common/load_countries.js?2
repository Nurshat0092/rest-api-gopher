var load_countries = function (cb) {
  var COUNTRIES = [];
  var req = new XMLHttpRequest();
  req.onreadystatechange = function () {
      if (this.readyState == 4 && this.status == 200) {
          cb(JSON.parse(this.responseText));
      }
  }
  req.open("GET", "/static/data/countries.json");
  req.send();
};